{
  "installCommand": "git clone https://github.com/flutter/flutter.git --depth 1 -b stable .flutter && ./.flutter/bin/flutter --version",
  "buildCommand": ".flutter/bin/flutter build web --release",
  "outputDirectory": "build/web",
  "rewrites": [
    {
      "source": "/(?!.*\\.(js|css|png|jpg|jpeg|svg|gif|json|webmanifest|wasm|data)$)(.*)",
      "destination": "/index.html"
    }
  ]
}
